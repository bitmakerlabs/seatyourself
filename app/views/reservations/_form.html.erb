<%= form_for ([restaurant, reservation]) do |f| %>


<div class="field">
  <%= f.label :time %>
  <%= f.time_field :time %>
</div>

<div class="field_1">
  <%= f.label :number_of_guests %>
  <%= f.number_field :number_of_guests %>
</div>

<div class="submit">
  <%= f.submit "Submit" %>
</div>

<% end %>
