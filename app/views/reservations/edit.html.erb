<h1>Reservation edit</h1>
<h1>found in views reservation.edit.html.erb</h1>

<%= form_for @reservation do |f| %>
  <div class="field">
    <p><%= f.label :time %></p>
    <p><%= f.text_field :time %></p>
  </div>

  <div class="field">
    <p><%= f.label :size %></p>
    <p><%= f.number_field :size %></p>
  </div>

  <div class="field">
    <p><%= f.submit  %></p>
  </div>
<% end %>
