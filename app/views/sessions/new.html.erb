<h1>Sign In</h1>

<%= form_tag session_path do %>

<div>
  <%=  label_tag :email %><br>
  <%=  text_field_tag :email %>
</div>

<div>
  <%=  label_tag :password %><br>
  <%=  password_field_tag :password %>
</div>

<div>
  <%=  submit_tag "Sign In" %>

</div>


<% end  %>

<p> <%= button_to "Create NEW ACCOUNT", new_user_path, :method => 'get'%> </p>
