<%= form_for restaurant do |f| %>

<p>
  <%= f.label :name %>
  <%= f.text_field :name %>
</p>

<p>
  <%= f.label :address %>
  <%= f.email_field :address %>
</p>

<p>
  <%= f.label :open_time %>
  <%= f.text_field :open_time %>
</p>

<p>
  <%= f.label :close_time %>
  <%= f.text_field :close_time %>
</p>

<p>
  <%= f.submit_tag %>
</p>

<% end %>
