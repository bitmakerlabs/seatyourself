<div class="container form-fix">
<h1>Login</h1>
<small><%= link_to "Need Account? Sign Up here", new_users_path%></small>
<%= form_for :session, url: session_path do %>
  <p>
    <%= label_tag :email %>
    <%= email_field_tag :email %>
  </p>
  <p>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </p>
  <p>
    <%= submit_tag "Log In"%>
  </p>

<% end %>
</div>
